<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:me="clr-namespace:MimaSim.MarkupExtensions" x:CompileBindings="False"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450" Name="register" Width="35" Height="25"
             x:Class="MimaSim.Controls.MimaComponents.RegisterControl">

  <Border BorderBrush="Black" BorderThickness="1">
    <Grid>
      <Button Content="{Binding ElementName=register, Path=Register}" Classes="clickable"  FontSize="15" VerticalAlignment="Center" Command="{me:OpenRegisterPopup}" />
    </Grid>
  </Border>
</UserControl>