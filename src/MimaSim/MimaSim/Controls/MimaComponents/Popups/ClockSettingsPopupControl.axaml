<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:c="clr-namespace:MimaSim.Controls" x:CompileBindings="False"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="MimaSim.Controls.MimaComponents.Popups.ClockSettingsPopupControl">
  <c:DialogControl IsCancelEnabled="True" CommandText="OK" Command="{Binding SetClockSettings, Mode=TwoWay}" Header="Taktgeber">
    <StackPanel>
      <TextBlock Text="Frequenz" />
      <NumericUpDown Value="{Binding Frequency, Mode=TwoWay}" />
    </StackPanel>
  </c:DialogControl>
</UserControl>