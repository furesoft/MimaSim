<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:me="clr-namespace:MimaSim.MarkupExtensions"
             xmlns:p="clr-namespace:MimaSim.Controls.MimaComponents.Popups;assembly=MimaSim"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450" Name="register" Width="35" Height="25"
             x:Class="MimaSim.Controls.MimaComponents.RegisterControl">

  <Border BorderBrush="Black" BorderThickness="1">
    <Grid>
      <TextBlock Text="{Binding ElementName=register, Path=Register}" TextAlignment="Center" FontSize="15" VerticalAlignment="Center" />
    </Grid>
  </Border>
</UserControl>