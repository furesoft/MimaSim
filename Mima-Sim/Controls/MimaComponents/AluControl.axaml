             xmlns:b="clr-namespace:MimaSim.Core;assembly=MimaSim"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="MimaSim.Controls.MimaComponents.AluControl" Width="125" Height="125">
