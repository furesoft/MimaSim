<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="MimaSim.App">
  <Application.Styles>
    <StyleInclude Source="avares://MimaSim/Styles.axaml" />
    <StyleInclude Source="avares://MimaSim/Styles/ContentDialogStyle.axaml" />
    <StyleInclude Source="avares://MimaSim/Styles/DialogControlStyle.axaml" />

    <FluentTheme Mode="Light" />
  </Application.Styles>

  <Application.Resources>
    <Image Source="/Resources/Icons/play.png" x:Key="play" />
    <Image Source="/Resources/Icons/pause.png" x:Key="pause" />
  </Application.Resources>
</Application>